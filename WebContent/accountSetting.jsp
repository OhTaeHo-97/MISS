<form class="ui form" action = "account_edit.mem" method = "POST" name = "editForm" id = "editForm" onsubmit = "return edit_check()">
  <h4 class="ui dividing header">Edit Account</h4>
  <div class="ui form">
     <div class="two fields">
	    <div class="field">
	     	 <label>ID</label>
	     	 <input type="text" name = "member_id" id = "member_id" value="${data.member_id}" readonly>
	    </div>
	    <div class="field">
     	</div>
    </div>
  </div>
  <div class="two fields">
    <div class="field">
     	 <label>Password</label>
     	 <input type="password" name = "member_pw" id = "member_pw">
    </div>
   	<div class="field">
     	 <label>Confirm Password</label>
     	<input type="password" name = "confirm_pw" id = "confirm_pw">
    </div>
  </div>
	 <br><br>
	<div class="field">
    	<label>Phone Number</label>
    	<input type="text" name = "phone" id = "phone" placeholder="Write it down except for -" value="${cvo.phoneNumber}">
  	</div>
     <div class="two fields">
	    <div class="field">
	     	 <label>NickName</label>
	     	 <input type="text" name = "nickname" id = "nickname" value="${cvo.nickname}">
	    </div>
	    <div class="field">
	    <label><br></label>
		   <button class="ui button">
		  		Duplication Confirm
		   </button>
	   </div>
    </div>
  	<div class="field">
    	<label>E-mail</label>
    	<input type="email" name = "email" id = "email" placeholder="sks232@naver.com" value="${cvo.email}">
  	</div>
  	<div class="field">
    	<label>Billing Address</label>
    	<input type="text" name = "address" id = "address" placeholder="Street Address" value="${cvo.address}">
  	</div>
  	  	<input type = "submit" class="ui secondary button" value = "Save">
<br>
</form>
